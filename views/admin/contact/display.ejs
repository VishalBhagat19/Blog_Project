<%- include('../../header.ejs')%>
  <section class="dashboard-wrap mtb-40">
    <div class="container">
      <div class="body-content">
        <div class="row">
          <div class="col-md-3">
            <div class="dash-left">
              <ul>
                <li class="active"><a href="/admin/dashboard">Dashboard</a></li>
                <li><a href="/admin/blogdisplay" class="active">Blogs</a></li>
                <li><a href="/admin/category">Categories</a></li>
                <li><a href="/admin/contactdisplay">Contact</a></li>
                <li><a href="/admin/about">About</a></li>
                <li><a href="/logout">Logout</a></li>
              </ul>
            </div>
          </div>
          <div class="col-md-9">
            <div class="dash-right">
              <div class="dash-header">
                <div class="dash-title">
                  <h1>contact</h1>
                </div>
              </div>
              <table class="table table-bordered table-striped">
                <tr>
                  <th style="color: aliceblue;">Serial.no</th>
                  <th style="color: aliceblue;">name</th>
                  <th style="color: aliceblue;">email</th>
                  <th style="color: aliceblue;">phone number</th>
                  <th style="color: aliceblue;">description</th>
                </tr>
                <% i=1 %>

                <% contact.forEach((b)=>{ %>
                    <tr>
                        <td style="color: aliceblue;">
                            <%= i++ %>
                        </td>
                        <td style="color: aliceblue;">
                           <%= b.name %>
                        </td>
                        <td style="color: aliceblue;">
                            <%= b.email %>
                        </td>
                        <td style="color: aliceblue;">
                          <%= b.phone %>
                        </td>
                        <td style="color: aliceblue;">
                            <%= b.description %>
                        </td>

                   <% }) %>
              </table>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
  <%- include('../../footer.ejs')%>